        <mxCell id="oXUYFk8hZLo8qg336Q9G-2" value="&lt;h1&gt;오디오포커스&lt;/h1&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="oXUYFk8hZLo8qg336Q9G-1">
